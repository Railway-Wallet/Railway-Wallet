{
  "name": "railway-reactjs",
  "version": "5.19.2",
  "private": true,
  "author": "Right to Privacy Foundation <mail@righttoprivacy.foundation>",
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "yarn react-shared && npm run reset-cache && craco start",
    "dev": "craco start",
    "reset-cache": "rm -rf node_modules/.cache",
    "build-dev": "PUBLIC_URL=/ NODE_ENV=development REACT_APP_DEV_BUILD=1 craco build",
    "build-stag": "PUBLIC_URL=/ env REACT_APP_STAG_BUILD=1 craco build",
    "build-prod": "PUBLIC_URL=/ craco build",
    "build-electron": "craco build",
    "test": "craco test",
    "eslint": "eslint src --ext .ts,.tsx --fix",
    "check-circular-deps": "madge --circular --extensions ts,tsx src/**/*",
    "lint": "npm run eslint; npm run tsc;",
    "tsc": "tsc --noEmit",
    "prettier": "git prettier ./src --write",
    "electron:start": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build:mac": "rm -rf dist && npm run build-electron && electron-builder --mac --x64 --arm64",
    "electron:build:windows": "rm -rf dist && npm run build-electron && electron-builder --windows --x64",
    "electron:build:linux": "rm -rf dist && npm run build-electron && electron-builder --linux --x64",
    "electron:build:linuxpluswindows": "rm -rf dist && npm run build-electron && electron-builder --windows --linux --x64",
    "postinstall": "node -e \"if(!process.env.SKIP_POSTINSTALL){process.exit(1)}\" || (electron-builder install-app-deps && patch-package && yarn react-shared && yarn lint)",
    "cypress:open": "cypress open",
    "react-shared": ":"
  },
  "dependencies": {
    "@0x/contract-addresses": "8.7.0",
    "@fingerprintjs/fingerprintjs": "^3.3.6",
    "@multiformats/multiaddr": "12.1.3",
    "@railgun-community/cookbook": "2.8.10",
    "@railgun-community/shared-models": "7.4.4",
    "@railgun-community/waku-broadcaster-client-web": "8.1.0",
    "@railgun-community/wallet": "10.2.5",
    "@railway-developer/actionsheet-react": "github:railway-developer/actionsheet-react",
    "@reduxjs/toolkit": "1.9.5",
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@types/level-js": "^4.0.2",
    "assert": "^2.0.0",
    "axios": "0.27.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "classnames": "^2.3.2",
    "constants-browserify": "^1.0.0",
    "crypto-js": "^4.1.1",
    "customize-cra": "^1.0.0",
    "debug": "^4.3.4",
    "dns.js": "^1.0.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "ethers": "github:Railgun-Community/ethers.js#v6.7.10",
    "events": "^3.3.0",
    "fs": "0.0.1-security",
    "html2canvas": "^1.4.1",
