{
  "name": "railway-node-bridge",
  "version": "0.0.1",
  "private": true,
  "author": "Right to Privacy Foundation <mail@righttoprivacy.foundation>",
  "license": "GPL-3.0-only",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn lint && yarn tsc",
    "compile-test": "yarn clean && tsc -p tsconfig.test.json && tsc-alias -p tsconfig.test.json",
    "test-coverage": "yarn compile-test && nyc mocha 'src/**/__tests__/*.test.ts'",
    "test": "yarn compile-test && mocha 'src/**/__tests__/*.test.ts'",
    "tsc": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "check-circular-deps": "madge --circular .",
    "eslint": "eslint src --ext .ts,.tsx --fix",
    "remove-optional-breaking-deps": "rm -rf node_modules/bufferutil/ && rm -rf node_modules/utf-8-validate/ && rm -rf node_modules/@railgun-community/curve25519-scalarmult-wasm && rm -rf node_modules/@railgun-community/poseidon-hash-wasm",
    "postinstall": "node -e \"if(!process.env.SKIP_POSTINSTALL){process.exit(1)}\" || (yarn remove-optional-breaking-deps && patch-package)",
    "lint": "yarn check-circular-deps; yarn eslint; yarn tsc"
  },
  "dependencies": {
    "@ethersproject/shims": "^5.7.0",
    "@railgun-community/curve25519-scalarmult-rsjs": "0.2.4",
    "@railgun-community/poseidon-hash-rsjs": "1.0.1",
    "@railgun-community/shared-models": "7.0.2",
    "@railgun-community/waku-broadcaster-client-node": "7.0.2",
    "@railgun-community/wallet": "9.0.0",
    "@railgun-privacy/native-prover": "https://ipfs-lb.b-cdn.net/ipfs/QmWRy7e57xSfeKfck6hLSFyJe9DNafpSZ3Af5bRFscHano/railgun-privacy-native-prover-4.0.2.tgz",
    "bindings-noderify-nodejs-mobile": "^10.5.0",
    "ethers": "github:Railgun-Community/ethers.js#v6.7.10",
    "leveldown-nodejs-mobile": "6.0.1-1",
    "no-op": "1.0.3",
    "nodejs-mobile-ipc2": "1.1.0",
    "urlpattern-polyfill-no-unicode": "7.0.0"
  },
  "engines": {
    "node": "18"
  },
  "devDependencies": {
    "@types/chai": "4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/leveldown": "^4.0.3",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "glob": "10.2.3",
    "madge": "6.0.0",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "patch-package": "7.0.0",
    "rimraf": "5.0.0",
    "ts-node": "^10.5.0",
    "tsc-alias": "^1.6.3",
    "typescript": "5.0.4"
  }
}
